{
  "name": "cozy-mobile",
  "description": "This is the native mobile client for Cozy.",
  "version": "0.1.3",
  "engines": {
    "node": "~0.10"
  },
  "scripts": {
    "postinstall": "npm run build",
    "build": "brunch build",
    "lint": "coffeelint -r app test *.coffee",
    "test": "mocha test/unit/* --reporter spec --compilers coffee:coffee-script/register --colors",
    "start": "brunch watch --server"
  },
  "dependencies": {
    "async": "1.5.2",
    "auto-reload-brunch": "1.8.1",
    "backbone": "1.2.3",
    "brunch": "1.8.5",
    "clean-css-brunch": "1.8.0",
    "coffee-script-brunch": "1.8.3",
    "css-brunch": "1.7.0",
    "jade-brunch": "1.8.2",
    "javascript-brunch": "1.8.0",
    "jquery": "1.9.1",
    "json-brunch": "1.5.4",
    "moment": "2.11.1",
    "moment-timezone": "0.5.0",
    "node-polyglot": "1.0.0 ",
    "pouchdb": "5.2.1",
    "stylus-brunch": "1.8.1",
    "uglify-js-brunch": "1.7.8",
    "underscore": "1.8.3",
    "backbone.statemachine": "0.2.7"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "coffee-script": "1.10.0",
    "coffeelint": "1.14.2",
    "memdown": "1.1.2",
    "mocha": "2.4.5",
    "mockery": "1.4.0"
  }
}
