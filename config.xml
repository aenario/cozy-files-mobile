<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="10305" android-versionName="1.3.5" id="io.cozy.files_client" ios-CFBundleIdentifier="io.cozy.files" version="1.3.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cozy</name>
    <description>Sync files from your cozycloud</description>
    <author email="contact@cozycloud.cc" href="https://cozy.io">Cozy Cloud</author>
    <preference name="Fullscreen" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="AndroidLaunchMode" value="singleTask" />
    <preference name="android-windowSoftInputMode" value="stateHidden|adjustPan" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <content src="index.html" />
    <platform name="android">
        <icon density="mdpi" src="res/icons/icon-48-mdpi.png" />
        <icon density="hdpi" src="res/icons/icon-72-hdpi.png" />
        <icon density="xhdpi" src="res/icons/icon-96-xhdpi.png" />
        <icon density="xxhdpi" src="res/icons/icon-144-xxhdpi.png" />
        <icon density="xxxhdpi" src="res/icons/icon-192-xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <icon height="120" src="res/icons/icon-120-ios.png" width="120" />
        <icon height="180" src="res/icons/icon-180-ios.png" width="180" />
        <icon height="76" src="res/icons/icon-76-ios.png" width="76" />
        <icon height="152" src="res/icons/icon-152-ios.png" width="152" />
        <icon height="167" src="res/icons/icon-167-ios.png" width="167" />
    </platform>
    <access origin="*" />
    <allow-intent href="mailto:*" />
    <plugin name="cordova-plugin-x-toast" spec="2.5.2" />
    <plugin name="cordova-plugin-whitelist" spec="1.2.2" />
    <plugin name="cordova-plugin-device" spec="1.1.2" />
    <plugin name="cordova-plugin-globalization" spec="https://github.com/StyrLabs/cordova-plugin-globalization.git" />
    <plugin name="cordova-plugin-file" spec="4.2.0" />
    <plugin name="cordova-plugin-file-transfer" spec="1.5.1" />
    <plugin name="cordova-plugin-battery-status" spec="1.1.2" />
    <plugin name="cordova-plugin-network-information" spec="1.2.1" />
    <plugin name="cordova-plugin-blob-constructor-polyfill" spec="1.0.2" />
    <plugin name="cordova-sqlite-storage" spec="1.4.5" />
    <plugin name="de.appplant.cordova.plugin.local-notification" spec="0.8.4" />
    <plugin name="io.cozy.cordova-images-browser" spec="https://github.com/cozy/cordova-images-browser" />
    <plugin name="io.cozy.jsbackgroundservice" spec="https://github.com/cozy/cordova-jsbackgroundservice" />
    <plugin name="io.cozy.jsbgservice-newpicture" spec="https://github.com/cozy/cordova-jsbgservice-newpicture" />
    <plugin name="io.cozy.contacts" spec="https://github.com/cozy/cordova-plugin-contacts" />
    <plugin name="io.cozy.calendarsync" spec="https://github.com/cozy/cordova-plugin-calendarsync" />
    <plugin name="cordova-plugin-file-opener2" spec="2.0.2" />
    <engine name="ios" spec="3.9.2" />
    <engine name="android" spec="4.1.1" />
</widget>
